{
  "name": "react-octopus",
  "version": "1.0.0-beta.2",
  "description": "Develop your React applications quickly and easily!",
  "author": "expert-m",
  "license": "MIT",
  "repository": "expert-m/react-octopus",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd examples/client && yarn install && yarn run build",
    "deploy": "gh-pages -d examples/client/build"
